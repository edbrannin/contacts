{% extends "main.html.j2" %}
{% block content %}
<h1>HERE BE PEOPLE.</h1>

<a href="{{ url_for('login') }}">Log in</a>

{% if 'me' in session or g.user %}
<h2>Session</h2>
Here's what Facebook told us:
<dl>
{% for key in g.user %}
<dt>{{key | e}}</dt>
<dd>{{g.user[key] | e}}</dd>
{% endfor %}
</dl>
{% endif %}

{% if logged_in %}
<contacts tags="{{tags}}"></contacts>
{% endif %}

{% endblock %}
