{% extends "main.html.j2" %}
{% block content %}
<h1>HERE BE PEOPLE.</h1>

<a href="{{ url_for('login') }}">Log in</a>

{% if 'me' in session %}
<h2>Session</h2>
Here's what Facebook told us:
<dl>
{% for key in session['me'] %}
<dt>{{key | e}}</dt>
<dd>{{session['me'][key] | e}}</dd>
{% endfor %}
</dl>
{% endif %}


{% endblock %}
